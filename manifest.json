{
    "name": "Sound Meters",
    "description": "A chrome extension that displays sound meters for a tab",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabCapture",
      "storage",
      "activeTab"
    ]
  }